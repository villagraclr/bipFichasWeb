@using SistemasBIPS.Models
@model EvaluacionExAnteViewModels
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Ministerio de Desarrollo Social y Familia - Gobierno de Chile";
}
@section styles {
    @Styles.Render("~/Content/Fonts_min")
    @Styles.Render("~/Content/DataTable")
}
<div id="page-wrapper">
    <div class="row">
        <ol class="breadcrumb breadcrumbBIPS">
            <li><a><i class="fa fa-folder-open-o fa-fw"></i> Ex-Ante</a></li>
            <li class="active"><i class="fa fa-calendar-check-o fa-fw"></i> Evaluaciones Ex-Ante</li>
        </ol>
    </div>
    <div class="row">
        <div class="col-lg-12" style="padding-left:0px;padding-right:0px">
            <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover" cellspacing="0" id="tblEvalExAnte">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Ministerio</th>
                            <th>Servicio</th>
                            <th>Nombre</th>
                            <th>Versión</th>
                            <th>Puntaje final</th>
                            <th>Calificación</th>
                            <th>Evaluador 1</th>
                            <th>Evaluador 2</th>
                            <th>Revisión Sectorialista</th>
                            <th>Revisión Monitoreo</th>
                            <th>Revisión Estudios</th>
                            <th>Finalizar Evaluación</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!--Modal evaluación-->
<div id="modalEvaluacion" class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Editar Evaluación</h4>
            </div>
            <div class="modal-body" style="height:450px;overflow-y:auto;">
                <input type="hidden" id="txtIdProgramaEvaluacion" value="0" />
                <div class="form-horizontal">
                    <div class="form-group">
                        <label for="cmbCalificacion" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Calificación</label>
                        <div class="col-sm-9" style="padding-left:15px;">
                            <select id="cmbCalificacion" class="form-control" style="width:90%;">
                                <option value="-1">Seleccione</option>
                                @if (Model.listaCalificaciones.Count > 0)
                                {
                                    foreach (var item in Model.listaCalificaciones)
                                    {
                                        <option value="@item.IdParametro">@item.Descripcion</option>
                                    }
                                }
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="txtComentSegpres" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Comentario SEGPRES</label>
                        <div class="col-sm-9" style="padding-left:15px;">
                            <textarea class="form-control validaEval" id="txtComentSegpres" rows="3" maxlength="8000" aria-describedby="txtCount14" placeholder="Ingrese comentario" style="width:90%;"></textarea>
                            <span id="txtCount1" class="help-block" style="font-size:12px;">Caracteres restantes=8000</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="txtComentGeneral" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Comentario General</label>
                        <div class="col-sm-9" style="padding-left:15px;">
                            <textarea class="form-control validaEval" id="txtComentGeneral" rows="3" placeholder="Ingrese comentario" style="width:90%;"></textarea>
                            <span id="txtCount2" class="help-block" style="font-size:12px;">Caracteres restantes=8000</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="txtAtingencia" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Atingencia</label>
                        <div class="col-sm-9" style="padding-left:15px;">
                            <textarea class="form-control validaEval" id="txtAtingencia" rows="3" placeholder="Ingrese texto" style="width:90%;"></textarea>
                            <span id="txtCount3" class="help-block" style="font-size:12px;">Caracteres restantes=8000</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="txtCoherencia" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Coherencia</label>
                        <div class="col-sm-9" style="padding-left:15px;">
                            <textarea class="form-control validaEval" id="txtCoherencia" rows="3" placeholder="Ingrese texto" style="width:90%;"></textarea>
                            <span id="txtCount4" class="help-block" style="font-size:12px;">Caracteres restantes=8000</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="txtConsistencia" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Consistencia</label>
                        <div class="col-sm-9" style="padding-left:15px;">
                            <textarea class="form-control validaEval" id="txtConsistencia" rows="3" placeholder="Ingrese texto" style="width:90%;"></textarea>
                            <span id="txtCount5" class="help-block" style="font-size:12px;">Caracteres restantes=8000</span>
                        </div>
                    </div>
                    <p class="bg-info" style="padding:8px;font-size:12px;">Comentarios por Ámbito de Evaluación</p>
                    <div class="form-group">
                        <label for="txtAntPrograma" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Antecedentes del Programa</label>
                        <div class="col-sm-9" style="padding-left:15px;">
                            <textarea class="form-control validaEval" id="txtAntPrograma" rows="3" placeholder="Ingrese texto" style="width:90%;"></textarea>
                            <span id="txtCount6" class="help-block" style="font-size:12px;">Caracteres restantes=8000</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="txtDiagPrograma" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Diagnóstico de la Necesidad del Programa</label>
                        <div class="col-sm-9" style="padding-left:15px;">
                            <textarea class="form-control validaEval" id="txtDiagPrograma" rows="3" placeholder="Ingrese texto" style="width:90%;"></textarea>
                            <span id="txtCount7" class="help-block" style="font-size:12px;">Caracteres restantes=8000</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="txtObjPobPrograma" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Objetivo del Programa y Población</label>
                        <div class="col-sm-9" style="padding-left:15px;">
                            <textarea class="form-control validaEval" id="txtObjPobPrograma" rows="3" placeholder="Ingrese texto" style="width:90%;"></textarea>
                            <span id="txtCount8" class="help-block" style="font-size:12px;">Caracteres restantes=8000</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="txtEstraPrograma" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Estrategia del Programa</label>
                        <div class="col-sm-9" style="padding-left:15px;">
                            <textarea class="form-control validaEval" id="txtEstraPrograma" rows="3" placeholder="Ingrese texto" style="width:90%;"></textarea>
                            <span id="txtCount9" class="help-block" style="font-size:12px;">Caracteres restantes=8000</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="txtIndicadores" class="col-sm-3 control-label" style="padding-left:0px; padding-right:0px;">Indicadores</label>
                        <div class="col-sm-9" style="padding-left:15px;">
