                <button type="button" class="btn btn-primary" data-loading-text="Guardando..." id="btnGuardarEvaluacion">Guardar</button>
                <button type="button" class="btn btn-success" data-loading-text="Enviando..." id="btnEnviarEvaluacion">Enviar</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
